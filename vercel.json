{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/proxy.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/relay.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "server.js"
    },
    {
      "src": "/api/proxy",
      "dest": "api/proxy.js"
    },
    {
      "src": "/api/relay",
      "dest": "api/relay.js"
    },
    {
      "src": "/steal.html",
      "dest": "/public/steal.html"
    },
    {
      "src": "/evil.css",
      "dest": "/public/evil.css"
    }
  ]
}
